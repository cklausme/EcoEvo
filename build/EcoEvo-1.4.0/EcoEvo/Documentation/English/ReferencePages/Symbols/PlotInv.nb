     SubscriptBox["n", "i"], "[", "t", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"dp", "[", "i_", "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"c", "[", 
          RowBox[{
           SubscriptBox["xn", "j"], ",", 
           SubscriptBox["xp", "i"]}], "]"}], "*", 
         RowBox[{
          SubscriptBox["n", "j"], "[", "t", "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          SubscriptBox["\[ScriptCapitalN]", "n"]}], "}"}]}], "]"}], "-", 
      "m"}], ")"}], "*", 
