Cell[BoxData[
 RowBox[{
  RowBox[{"SetModel", "[", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"Guild", "[", "n", "]"}], "\[Rule]", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Equation", "\[RuleDelayed]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"r", "[", 
             SubscriptBox["xn", "\[FormalI]"], "]"}], "-", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"a", "[", 
                RowBox[{
                 SubscriptBox["xn", "\[FormalI]"], ",", 
                 SubscriptBox["xn", "\[FormalJ]"]}], "]"}], 
               SubscriptBox["n", "\[FormalJ]"]}], ",", 
              RowBox[{"{", 
               RowBox[{"\[FormalJ]", ",", 
                SubscriptBox["\[ScriptCapitalN]", "n"]}], "}"}]}], "]"}], "-", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"c", "[", 
                RowBox[{
                 SubscriptBox["xn", "\[FormalI]"], ",", 
                 SubscriptBox["xp", "\[FormalJ]"]}], "]"}], 
               SubscriptBox["p", "\[FormalJ]"]}], ",", 
              RowBox[{"{", 
               RowBox[{"\[FormalJ]", ",", 
                SubscriptBox["\[ScriptCapitalN]", "p"]}], "}"}]}], "]"}]}], 
           ")"}], 
          SubscriptBox["n", "\[FormalI]"]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"Traits", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], "\[LessEqual]", "xn", "\[LessEqual]", "1"}], 
          "}"}]}]}], "}"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Guild", "[", "p", "]"}], "\[Rule]", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Equation", "\[RuleDelayed]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"c", "[", 
                RowBox[{
                 SubscriptBox["xn", "\[FormalJ]"], ",", 
                 SubscriptBox["xp", "\[FormalI]"]}], "]"}], 
               SubscriptBox["n", "\[FormalJ]"]}], ",", 
              RowBox[{"{", 
               RowBox[{"\[FormalJ]", ",", 
                SubscriptBox["\[ScriptCapitalN]", "n"]}], "}"}]}], "]"}], "-",
             "m"}], ")"}], 
          SubscriptBox["p", "\[FormalI]"]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"Traits", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], "\[LessEqual]", "xp", "\[LessEqual]", "1"}], 
          "}"}]}]}], "}"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"Parameters", "\[RuleDelayed]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Sigma]", ">", "0"}], ",", 
        RowBox[{"\[Sigma]c", ">", "0"}], ",", 
        RowBox[{"m", ">", "0"}]}], "}"}]}]}], "\[IndentingNewLine]", "}"}], 
   "]"}], ";"}]], "Input",
