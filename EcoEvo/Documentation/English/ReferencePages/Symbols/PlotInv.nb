Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"SetModel", "[", 
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Guild", "[", "n", "]"}], "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Equation", "\[Rule]", "g"}], ",", 
        RowBox[{
         RowBox[{"Trait", "[", "x", "]"}], "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Range", "\[Rule]", 
           RowBox[{"Interval", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "}"}], "]"}]}], "}"}]}]}], 
       "}"}]}], "\[IndentingNewLine]", "}"}], "]"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "i_", "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"r", "[", 
       SubscriptBox["x", "i"], "]"}], "-", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"a", "[", 
          RowBox[{
           SubscriptBox["x", "i"], ",", 
           SubscriptBox["x", "j"]}], "]"}], "*", 
         RowBox[{
          SubscriptBox["n", "j"], "[", "t", "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          SubscriptBox["\[ScriptCapitalN]", "n"]}], "}"}]}], "]"}]}], ")"}], 
    "*", 
    RowBox[{
     SubscriptBox["n", "i"], "[", "t", "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", 
    RowBox[{"xi_", ",", "xj_"}], "]"}], ":=", 
   RowBox[{"E", "^", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "/", "2"}], "*", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"xi", "-", "xj"}], ")"}], "^", "2"}], "/", 
       RowBox[{"\[Sigma]", "^", "2"}]}]}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"r", "[", "x_", "]"}], ":=", 
   RowBox[{"1", "-", 
    RowBox[{"x", "^", "2"}]}]}], ";"}], "\[IndentingNewLine]", 
