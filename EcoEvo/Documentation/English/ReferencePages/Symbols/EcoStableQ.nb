 RowBox[{
  RowBox[{
   RowBox[{"rm", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Pop", "[", "1", "]"}], "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Variable", "\[Rule]", "n"}], ",", 
         RowBox[{"Equation", "\[RuleDelayed]", "dN"}], ",", 
         RowBox[{"Color", "\[Rule]", "Green"}]}], "}"}]}], ",", 
      RowBox[{
       RowBox[{"Pop", "[", "2", "]"}], "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Variable", "\[Rule]", "p"}], ",", 
         RowBox[{"Equation", "\[RuleDelayed]", "dP"}], ",", 
         RowBox[{"Color", "\[Rule]", "Red"}]}], "}"}]}]}], "}"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dN", ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"n", "[", "t", "]"}], "*", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        RowBox[{"n", "[", "t", "]"}], "/", "k"}]}], ")"}]}], "-", 
    RowBox[{"an", " ", 
